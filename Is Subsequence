 bool isSubsequence(string s, string t) {
        int i=0,j=0;
        while(j<s.length() && i<t.length())
        {
            if(s[j]==t[i])
            {
                j++;
                i++;
            }
            else
                i++;
        }
        if(j==s.length())
            return true;
        else
            return false;
    }
